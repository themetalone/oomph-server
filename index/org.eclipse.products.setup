<?xml version="1.0" encoding="UTF-8"?>
<setup:ProductCatalog
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:setup="http://www.eclipse.org/oomph/setup/1.0"
    xmlns:setup.p2="http://www.eclipse.org/oomph/setup/p2/1.0"
    name="org.eclipse.products"
    label="Eclipse.org">
  <annotation
      source="http://www.eclipse.org/oomph/setup/BrandingInfo">
    <detail
        key="readmePath">
      <value>readme/readme_eclipse.html</value>
    </detail>
  </annotation>
  <annotation
      source="http://www.eclipse.org/oomph/setup/StatsSending">
    <detail
        key="uri">
      <value>http://download.eclipse.org/stats/oomph</value>
    </detail>
  </annotation>
  <setupTask
      xsi:type="setup:InstallationTask"
      id="installation"/>
  <setupTask
      xsi:type="setup.p2:P2Task">
    <requirement
        name="org.eclipse.oomph.setup.feature.group"/>
    <repository
        url="${oomph.update.url}"/>
  </setupTask>

  <product
    xmlns:com.github.maybeec.oomph.task="http://maybeec.github.io/oomph-task-download/1.0"
    xmlns:com.github.maybeec.oomph.task_1="http://maybeec.github.io/oomph-task-unzip/1.0"
    xmlns:com.github.maybeec.oomph.task_2="http://maybeec.github.io/oomph-task-cli/1.0"
    xmlns:jdt="http://www.eclipse.org/oomph/setup/jdt/1.0"
    xsi:schemaLocation="http://maybeec.github.io/oomph-task-download/1.0 https://raw.githubusercontent.com/maybeec/oomph-task-download/master/task-download-updatesite/model/Oomph-task-download.ecore http://maybeec.github.io/oomph-task-unzip/1.0 https://raw.githubusercontent.com/maybeec/oomph-task-unzip/master/task-unzip-updatesite/model/Oomph-task-unzip.ecore http://maybeec.github.io/oomph-task-cli/1.0 https://raw.githubusercontent.com/maybeec/oomph-task-cli/master/task-cli-updatesite/model/Oomph-cli-task-1.0.ecore http://www.eclipse.org/oomph/setup/jdt/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/JDT.ecore"
    name="oasp4j"
    label="OASP4J">
  <annotation
      source="http://www.eclipse.org/oomph/setup/BrandingInfo">
    <detail
        key="folderName">
      <value>eclipse</value>
    </detail>
    <detail
        key="folderName.macosx">
      <value>Eclipse</value>
    </detail>
  </annotation>
  <setupTask
      xsi:type="setup:CompoundTask"
      id="config.user.input"
      name="unset config as simple user interface">
    <setupTask
        xsi:type="setup:VariableTask"
        id="installation.root"
        type="FOLDER"
        name="Root installation path"
        storageURI="scope://Installation"
        label="">
      <description>Installation Folder</description>
    </setupTask>
  </setupTask>
  <setupTask
      xsi:type="setup:CompoundTask"
      id="config.versions.noneclipse.resources"
      name="version config of non-eclipse resources">
    <setupTask
        xsi:type="setup:VariableTask"
        id="oasp.scripts.version"
        name="oasp.scripts.version"
        defaultValue="1.3.1"
        storageURI="scope://Installation"
        label="oasp4j-scripts version">
      <description>OASP Scripts version to be used</description>
    </setupTask>
    <setupTask
        xsi:type="setup:VariableTask"
        id="ant.version"
        name="ant.version"
        defaultValue="1.9.7"
        storageURI="scope://Installation"
        label="ant version">
      <description>ANT version to be installed</description>
    </setupTask>
    <setupTask
        xsi:type="setup:VariableTask"
        id="elasticsearch.version"
        name="elasticsearch.version"
        defaultValue="2.3.0"
        storageURI="scope://Installation"
        label="elasticsearch version">
      <description>ElasticSearch version to be installed</description>
    </setupTask>
    <setupTask
        xsi:type="setup:VariableTask"
        id="logstash.version"
        name="logstash.version"
        defaultValue="2.3.0"
        storageURI="scope://Installation"
        label="logstash version">
      <description>LogStash version to be installed</description>
    </setupTask>
    <setupTask
        xsi:type="setup:VariableTask"
        id="maven.version"
        name="maven.version"
        defaultValue="3.3.9"
        storageURI="scope://Installation"
        label="maven version">
      <description>Maven version to be installed</description>
    </setupTask>
    <setupTask
        xsi:type="setup:VariableTask"
        id="tomcat.version"
        name="tomcat.version"
        defaultValue="7.0.8"
        storageURI="scope://Installation"
        label="tomcat version">
      <description>Tomcat version to be installed</description>
    </setupTask>
    <setupTask
        xsi:type="setup:VariableTask"
        id="python.version"
        name="python.version"
        defaultValue="2.7.8"
        storageURI="scope://Installation"
        label="python version">
      <description>Python Version to be installed</description>
    </setupTask>
    <setupTask
        xsi:type="setup:VariableTask"
        id="nodejs.version"
        name="nodejs.version"
        defaultValue="4.4.5"
        storageURI="scope://Installation"
        label="nodejs version">
      <description>NodeJS version to be installed</description>
    </setupTask>
    <description>Configuration of product versions of non-eclipse resources</description>
  </setupTask>
  <setupTask
      xsi:type="setup:CompoundTask"
      id="config.common.paths"
      name="config of commonly used paths">
    <setupTask
        xsi:type="setup:VariableTask"
        id="software.location"
        name="software.location"
        value="${installation.root/software}"
        storageURI="scope://Installation"/>
    <setupTask
        xsi:type="setup:VariableTask"
        name="workspaces.location"
        value="${installation.root/workspaces}"
        storageURI="scope://Installation"
        label=""/>
    <setupTask
        xsi:type="setup:VariableTask"
        name="settings.location"
        value="${workspaces.location/main}"
        storageURI="scope://Installation"/>
    <setupTask
        xsi:type="setup:VariableTask"
        name="tmp.download.location"
        value="${installation.root/temp}"
        storageURI="scope://Installation">
      <description>Specifies where temporary data will be stored</description>
    </setupTask>
    <setupTask
        xsi:type="setup:VariableTask"
        name="workspace.location"
        value="${workspaces.location/master}"
        storageURI="scope://Workspace"/>
    <description>Configuration of commonly used paths</description>
  </setupTask>
  <setupTask
      xsi:type="setup:CompoundTask"
      id="config.reserved.paths"
      name="config of paths overriding reserved oomph variables">
    <setupTask
        xsi:type="setup:VariableTask"
        id="installation.location"
        name="installation.location"
        value="${software.location}"
        storageURI="scope://Installation"/>
  </setupTask>
  <setupTask
      xsi:type="setup:CompoundTask"
      id="compound.downloads"
      name="download tasks of external resources">
    <setupTask
        xsi:type="com.github.maybeec.oomph.task:downloadTask"
        id="download.ant"
        excludedTriggers="STARTUP"
        filter=""
        resourceLocation="http://archive.apache.org/dist/ant/binaries/apache-ant-${ant.version}-bin.zip"
        isURL="true"
        readTimeout="5000"
        destinationPath="${tmp.download.location}"
        destinationFile="apache-ant-${ant.version}-bin.zip"
        priority="100"
        estimatedSize="8">
      <description>Downloads the ant binaries as zip file</description>
    </setupTask>
    <setupTask
        xsi:type="com.github.maybeec.oomph.task:downloadTask"
        id="download.elasticsearch"
        excludedTriggers="STARTUP"
        filter=""
        resourceLocation="https://download.elasticsearch.org/elasticsearch/release/org/elasticsearch/distribution/zip/elasticsearch/${elasticsearch.version}/elasticsearch-${elasticsearch.version}.zip"
        isURL="true"
        readTimeout="5000"
        destinationPath="${tmp.download.location}"
        destinationFile="elasticsearch-${elasticsearch.version}.zip"
        priority="100"
        estimatedSize="27">
      <description>Downloads the elasticsearch binaries as zip file</description>
    </setupTask>
    <setupTask
        xsi:type="com.github.maybeec.oomph.task:downloadTask"
        id="download.logstash"
        excludedTriggers="STARTUP"
        filter=""
        resourceLocation="https://download.elastic.co/logstash/logstash/logstash-${logstash.version}.zip"
        isURL="true"
        readTimeout="5000"
        destinationPath="${tmp.download.location}"
        destinationFile="logstash-${logstash.version}.zip"
        priority="100"
        estimatedSize="77">
      <description>Downloads the logstash binaries as zip file</description>
    </setupTask>
    <setupTask
        xsi:type="com.github.maybeec.oomph.task:downloadTask"
        id="download.maven"
        excludedTriggers="STARTUP"
        filter=""
        resourceLocation="http://www-us.apache.org/dist/maven/maven-3/${maven.version}/binaries/apache-maven-${maven.version}-bin.zip"
        isURL="true"
        readTimeout="5000"
        destinationPath="${tmp.download.location}"
        destinationFile="apache-maven-${maven.version}-bin.zip"
        priority="100"
        estimatedSize="8">
      <description>Downloads the maven binaries as a zip file</description>
    </setupTask>
    <setupTask
        xsi:type="com.github.maybeec.oomph.task:downloadTask"
        id="download.oasp"
        excludedTriggers="STARTUP"
        filter=""
        resourceLocation="https://github.com/oasp/oasp4j-ide/archive/master.zip"
        isURL="true"
        readTimeout="5000"
        destinationPath="${tmp.download.location}"
        destinationFile="oasp4j.zip"
        priority="100">
      <description>Downloads the main zip file for the OASP4J</description>
    </setupTask>
    <setupTask
        xsi:type="setup:CompoundTask"
        id="tasks.download.external.ressources.windows"
        filter="(os.name=Windows*)"
        name="windows">
      <setupTask
          xsi:type="com.github.maybeec.oomph.task:downloadTask"
          id="download.oasp.scripts"
          excludedTriggers="STARTUP"
          filter=""
          resourceLocation="http://repo1.maven.org/maven2/io/oasp/java/ide/oasp4j-ide-scripts/${oasp.scripts.version}/oasp4j-ide-scripts-${oasp.scripts.version}.zip"
          isURL="true"
          readTimeout="5000"
          destinationPath="${tmp.download.location}"
          destinationFile="oasp4j-ide-scripts-${oasp.scripts.version}.zip"
          priority="100">
        <description>Downloads the scripts for the OASP4j</description>
      </setupTask>
      <setupTask
          xsi:type="com.github.maybeec.oomph.task:downloadTask"
          id="download.win.help"
          excludedTriggers="STARTUP"
          filter=""
          resourceLocation="https://raw.githubusercontent.com/themetalone/oomph-server/master/help.bat"
          isURL="true"
          readTimeout="5000"
          destinationPath="${installation.root}"
          destinationFile="help.bat"
          priority="100">
        <description>Downloads a helper script</description>
      </setupTask>
      <setupTask
          xsi:type="setup:CompoundTask"
          id="tasks.download.external.ressources.windows.x64"
          filter="(os.arch=amd64)"
          name="x64">
        <setupTask
            xsi:type="com.github.maybeec.oomph.task:downloadTask"
            id="download.tomcat.win64"
            excludedTriggers="STARTUP"
            filter=""
            resourceLocation="http://archive.apache.org/dist/tomcat/tomcat-7/v${tomcat.version}/bin/apache-tomcat-${tomcat.version}-windows-x64.zip"
            isURL="true"
            readTimeout="5000"
            destinationPath="${tmp.download.location}"
            destinationFile="apache-tomcat-${tomcat.version}.zip"
            priority="100"
            estimatedSize="9">
          <description>Downloads the tomcat binaries as zip file</description>
        </setupTask>
        <setupTask
            xsi:type="com.github.maybeec.oomph.task:downloadTask"
            id="download.win.nodejs"
            excludedTriggers="STARTUP"
            filter=""
            resourceLocation="https://nodejs.org/dist/v${nodejs.version}/node-v${nodejs.version}-x64.msi"
            isURL="true"
            readTimeout="5000"
            destinationPath="${tmp.download.location}"
            destinationFile="node-v${nodejs.version}.msi"
            priority="100"
            estimatedSize="6">
          <description>Downloads a nodejs installer</description>
        </setupTask>
        <setupTask
            xsi:type="com.github.maybeec.oomph.task:downloadTask"
            id="download.win.python"
            excludedTriggers="STARTUP"
            filter=""
            resourceLocation="https://www.python.org/ftp/python/${python.version}/python-${python.version}.amd64.msi"
            isURL="true"
            readTimeout="5000"
            destinationPath="${tmp.download.location}"
            destinationFile="python-${python.version}.msi"
            priority="100"
            estimatedSize="225">
          <description>Downloads the python installable</description>
        </setupTask>
        <description>Download tasks for external ressources on windows x64</description>
      </setupTask>
      <setupTask
          xsi:type="setup:CompoundTask"
          id="tasks.download.external.ressources.windows.x32"
          filter="(os.arch=x86)"
          name="x32">
        <setupTask
            xsi:type="com.github.maybeec.oomph.task:downloadTask"
            id="download.tomcat.win32"
            excludedTriggers="STARTUP"
            filter=""
            resourceLocation="http://archive.apache.org/dist/tomcat/tomcat-7/v${tomcat.version}/bin/apache-tomcat-${tomcat.version}-windows-x32.zip"
            isURL="true"
            readTimeout="5000"
            destinationPath="${tmp.download.location}"
            destinationFile="apache-tomcat-${tomcat.version}.zip"
            priority="100"
            estimatedSize="9">
          <description>Downloads the tomcat binaries as zip file</description>
        </setupTask>
        <description></description>
      </setupTask>
      <description>Download tasks for external ressources on windows</description>
    </setupTask>
    <description>downloads external resources</description>
  </setupTask>
  <setupTask
      xsi:type="setup:CompoundTask"
      id="tasks.unzip.external.resources"
      name="unzip tasks of external resources">
    <setupTask
        xsi:type="com.github.maybeec.oomph.task_1:unzipTask"
        id="unzip.oasp"
        excludedTriggers="STARTUP"
        predecessor="download.oasp"
        zipFile="${tmp.download.location/}oasp4j.zip"
        destinationDir="${tmp.download.location}"
        priority="100">
      <description>Unzips the oasp into the supposed folder</description>
    </setupTask>
    <setupTask
        xsi:type="com.github.maybeec.oomph.task_1:unzipTask"
        id="unzip.oasp.scripts"
        excludedTriggers="STARTUP"
        predecessor="download.oasp.scripts"
        zipFile="${tmp.download.location/}oasp4j-ide-scripts-${oasp.scripts.version}.zip"
        destinationDir="${installation.root}"
        priority="100">
      <description></description>
    </setupTask>
    <setupTask
        xsi:type="com.github.maybeec.oomph.task_1:unzipTask"
        id="unzip.ant"
        excludedTriggers="STARTUP"
        predecessor="download.ant"
        zipFile="${tmp.download.location/}apache-ant-${ant.version}-bin.zip"
        destinationDir="${software.location}"
        priority="100">
      <description>Unzips the ant binaries</description>
    </setupTask>
    <setupTask
        xsi:type="com.github.maybeec.oomph.task_1:unzipTask"
        id="unzip.elasticsearch"
        excludedTriggers="STARTUP"
        predecessor="download.elasticsearch"
        zipFile="${tmp.download.location/}elasticsearch-${elasticsearch.version}.zip"
        destinationDir="${software.location}"
        priority="100">
      <description>unzips the elasticsearch binaries</description>
    </setupTask>
    <setupTask
        xsi:type="com.github.maybeec.oomph.task_1:unzipTask"
        id="unzip.logstash"
        excludedTriggers="STARTUP"
        predecessor="download.logstash"
        zipFile="${tmp.download.location/}logstash-${logstash.version}.zip"
        destinationDir="${software.location}"
        priority="100">
      <description>Unzips the logstash binaries</description>
    </setupTask>
    <setupTask
        xsi:type="com.github.maybeec.oomph.task_1:unzipTask"
        id="unzip.maven"
        excludedTriggers="STARTUP"
        predecessor="download.maven"
        zipFile="${tmp.download.location/}apache-maven-${maven.version}-bin.zip"
        destinationDir="${software.location}"
        priority="100">
      <description>Unzips the maven binaries</description>
    </setupTask>
    <setupTask
        xsi:type="com.github.maybeec.oomph.task_1:unzipTask"
        id="unzip.tomcat"
        excludedTriggers="STARTUP"
        predecessor="download.tomcat.win64"
        zipFile="${tmp.download.location/}apache-tomcat-${tomcat.version}.zip"
        destinationDir="${software.location}"
        priority="100">
      <description>Unzips the tomcat binaries</description>
    </setupTask>
    <description></description>
  </setupTask>
  <setupTask
      xsi:type="setup:CompoundTask"
      id="tasks.finalize.external.resources"
      name="finalize external resources">
    <setupTask
        xsi:type="setup:CompoundTask"
        id="tasks.finalize.external.resources.win"
        filter="(os.name=Windows*)"
        name="Windows">
      <setupTask
          xsi:type="com.github.maybeec.oomph.task_2:cliTask"
          id="rename.ant"
          excludedTriggers="STARTUP"
          predecessor="unzip.ant"
          command="rename"
          priority="100">
        <argument>${software.location/}apache-ant-${ant.version}</argument>
        <argument>ant</argument>
        <description>renames the ant directory to match the oasps settings</description>
      </setupTask>
      <setupTask
          xsi:type="com.github.maybeec.oomph.task_2:cliTask"
          id="rename.maven"
          excludedTriggers="STARTUP"
          predecessor="unzip.maven"
          command="rename"
          priority="100">
        <argument>${software.location/}apache-maven-${maven.version}</argument>
        <argument>maven</argument>
        <description>Rename the maven directory to plain maven</description>
      </setupTask>
      <setupTask
          xsi:type="com.github.maybeec.oomph.task_2:cliTask"
          id="rename.tomcat"
          excludedTriggers="STARTUP"
          predecessor="unzip.tomcat"
          command="rename"
          priority="100">
        <argument>${software.location/}apache-tomcat-${tomcat.version}</argument>
        <argument>tomcat</argument>
        <description>renames the tomcat directory</description>
      </setupTask>
      <setupTask
          xsi:type="com.github.maybeec.oomph.task_2:cliTask"
          id="rename.elasticsearch"
          excludedTriggers="STARTUP"
          predecessor="unzip.elasticsearch"
          command="rename"
          priority="100">
        <argument>${software.location/}elasticsearch-${elasticsearch.version}</argument>
        <argument>elasticsearch</argument>
      </setupTask>
      <setupTask
          xsi:type="com.github.maybeec.oomph.task_2:cliTask"
          id="rename.logstash"
          excludedTriggers="STARTUP"
          predecessor="unzip.logstash"
          command="rename"
          priority="100">
        <argument>${software.location/}logstash-${logstash.version}</argument>
        <argument>logstash</argument>
        <description></description>
      </setupTask>
      <setupTask
          xsi:type="com.github.maybeec.oomph.task_2:cliTask"
          id="mkdir.settings.location"
          excludedTriggers="STARTUP"
          predecessor="unzip.oasp.scripts"
          command="mkdir"
          priority="100">
        <argument>${workspaces.location/main/development}</argument>
      </setupTask>
      <setupTask
          xsi:type="com.github.maybeec.oomph.task_2:cliTask"
          id="move.settings.location"
          excludedTriggers="STARTUP"
          predecessor="mkdir.settings.location"
          command="move"
          priority="100">
        <argument>${tmp.download.location/oasp4j-ide-master/oasp4j-ide-settings/src/main/settings}</argument>
        <argument>${settings.location/development/}</argument>
      </setupTask>
      <setupTask
          xsi:type="com.github.maybeec.oomph.task_2:cliTask"
          id="mkdir.nodejs.folder"
          excludedTriggers="STARTUP"
          predecessor="download.win.nodejs"
          command="mkdir"
          priority="100">
        <argument>${software.location/nodejs}</argument>
      </setupTask>
      <setupTask
          xsi:type="com.github.maybeec.oomph.task_2:cliTask"
          id="install.nodejs"
          excludedTriggers="STARTUP"
          command="msiexec"
          priority="100">
        <argument>/a</argument>
        <argument>&quot;${tmp.download.location/}node-v${nodejs.version}.msi&quot;</argument>
        <argument>/qn</argument>
        <argument>TARGETDIR=&quot;${software.location/nodejs/}&quot;</argument>
        <description>Extracts NodeJS</description>
      </setupTask>
      <setupTask
          xsi:type="com.github.maybeec.oomph.task_2:cliTask"
          id="install.nodejs.reordering"
          excludedTriggers="STARTUP"
          command="xcopy"
          priority="100">
        <argument>&quot;${software.location/nodejs/nodejs}&quot;</argument>
        <argument>&quot;${software.location/nodejs}&quot;</argument>
        <argument>/s</argument>
        <argument>/e</argument>
        <argument>/i</argument>
        <argument>/h</argument>
        <argument>/y</argument>
      </setupTask>
      <setupTask
          xsi:type="com.github.maybeec.oomph.task_2:cliTask"
          id="install.nodejs.cleanup"
          excludedTriggers="STARTUP"
          command="rmdir"
          priority="100">
        <argument>&quot;${software.location/nodejs/nodejs}&quot;</argument>
        <argument>/s</argument>
        <argument>/q</argument>
      </setupTask>
      <setupTask
          xsi:type="com.github.maybeec.oomph.task_2:cliTask"
          id="install.nodejs.cleanup.msi"
          excludedTriggers="STARTUP"
          command="del"
          priority="100">
        <argument>&quot;${software.location/nodejs/}node-v${nodejs.version}.msi&quot;</argument>
      </setupTask>
      <setupTask
          xsi:type="com.github.maybeec.oomph.task_2:cliTask"
          id="mkdir.python.folder"
          excludedTriggers="STARTUP"
          command="mkdir"
          priority="100">
        <argument>${software.location/python}</argument>
      </setupTask>
      <setupTask
          xsi:type="com.github.maybeec.oomph.task_2:cliTask"
          id="install.python"
          excludedTriggers="STARTUP"
          filter=""
          command="msiexec"
          priority="100">
        <argument>/a</argument>
        <argument>&quot;${tmp.download.location/}python-${python.version}.msi&quot;</argument>
        <argument>/qn</argument>
        <argument>TARGETDIR=&quot;${software.location/python/}&quot;</argument>
        <description>Install MSI Python Package</description>
      </setupTask>
      <setupTask
          xsi:type="com.github.maybeec.oomph.task_2:cliTask"
          id="install.python.cleanup"
          excludedTriggers="STARTUP"
          command="del"
          priority="100">
        <argument>&quot;${software.location/python/}python-${python.version}.msi&quot;</argument>
      </setupTask>
      <setupTask
          xsi:type="com.github.maybeec.oomph.task_2:cliTask"
          id="mkdir.software.java"
          excludedTriggers="STARTUP"
          predecessor="config.versions.noneclipse.resources"
          command="mkdir"
          priority="100">
        <argument>${software.location/java/}</argument>
      </setupTask>
      <setupTask
          xsi:type="com.github.maybeec.oomph.task_2:cliTask"
          id="xcopy.java"
          excludedTriggers="STARTUP"
          predecessor="mkdir.software.java"
          command="xcopy"
          priority="100">
        <argument>${jre.location-1.7/}*</argument>
        <argument>&quot;${software.location/java/}&quot;</argument>
        <argument>/s</argument>
        <argument>/e</argument>
        <argument>/i</argument>
        <argument>/h</argument>
        <argument>/y</argument>
      </setupTask>
      <setupTask
          xsi:type="com.github.maybeec.oomph.task_2:cliTask"
          id="update.workspaces"
          excludedTriggers="STARTUP"
          predecessor="unzip.oasp.scripts"
          filter=""
          command="chdir"
          priority="1000">
        <argument>/D</argument>
        <argument>${installation.root}</argument>
        <argument>&amp;&amp;</argument>
        <argument>help.bat</argument>
      </setupTask>

  </setupTask>
  <setupTask
      xsi:type="setup:VariableTask"
      name="workspace.location"
      value="${workspace.container}/master"
      storageURI="scope://Workspace"/>
  <setupTask
      xsi:type="downloadtask:DownloadTask"
      id="download.oasp"
      resourceLocation="https://github.com/oasp/oasp4j-ide/archive/master.zip"
      isURL="true"
      destinationPath="${temp.download}"
      destinationFile="oasp.zip"/>
  <setupTask
      xsi:type="unziptask:UnzipTask"
      id="unzip.oasp"
      zipFile="${temp.download}/oasp.zip"
      destinationDir="${temp.download}"/>
  <setupTask
      xsi:type="downloadtask:DownloadTask"
      id="download.ant"
      excludedTriggers="STARTUP"
      resourceLocation="http://archive.apache.org/dist/ant/binaries/apache-ant-1.9.7-bin.zip"
      isURL="true"
      readTimeout="1000"
      destinationPath="${temp.download}"
      destinationFile="ant.zip"
      estimatedSize="8">
    <description>Downloads the ant 1.9.7 archive</description>
  </setupTask>
  <setupTask
      xsi:type="unziptask:UnzipTask"
      id="unzip.ant"
      excludedTriggers="STARTUP"
      predecessor="download.ant"
      filter=""
      zipFile="${temp.download}/ant.zip"
      destinationDir="${installation.location}">
    <description>Unzips the ant archive</description>
  </setupTask>
  <setupTask
      xsi:type="downloadtask:DownloadTask"
      id="download.elasticsearch"
      excludedTriggers="STARTUP"
      resourceLocation="https://download.elasticsearch.org/elasticsearch/release/org/elasticsearch/distribution/zip/elasticsearch/2.3.0/elasticsearch-2.3.0.zip"
      isURL="true"
      readTimeout="2000"
      destinationPath="${temp.download}"
      destinationFile="elasticsearch.zip"
      estimatedSize="27">
    <description>Download the elasticsearch 2.3 archive</description>
  </setupTask>
  <setupTask
      xsi:type="unziptask:UnzipTask"
      id="unzip.elasticsearch"
      excludedTriggers="STARTUP"
      predecessor="download.elasticsearch"
      zipFile="${temp.download}/elasticsearch.zip"
      destinationDir="${installation.location}"/>
  <setupTask
      xsi:type="downloadtask:DownloadTask"
      id="download.logstash"
      excludedTriggers="STARTUP"
      resourceLocation="https://download.elastic.co/logstash/logstash/logstash-2.3.0.zip"
      isURL="true"
      destinationPath="${temp.download}"
      destinationFile="logstash.zip"
      estimatedSize="77">
    <description>Downloads logstash 2.3</description>
  </setupTask>
  <setupTask
      xsi:type="unziptask:UnzipTask"
      id="unzip.logstash"
      excludedTriggers="STARTUP"
      predecessor="download.logstash"
      zipFile="${temp.download}/logstash.zip"
      destinationDir="${installation.location}"/>
  <setupTask
      xsi:type="downloadtask:DownloadTask"
      id="download.nodejs"
      excludedTriggers="STARTUP"
      resourceLocation="https://nodejs.org/dist/v4.4.2/win-x64/node.exe"
      isURL="true"
      destinationPath="${temp.download}"
      destinationFile="nodejs-4-4-2-x64.exe"
      estimatedSize="14">
    <description>Downloads the nodejs 4.4.2  x64 exe</description>
  </setupTask>
  <setupTask
      xsi:type="downloadtask:DownloadTask"
      id="download.python"
      excludedTriggers="STARTUP"
      resourceLocation="https://www.python.org/ftp/python/2.7.11/python-2.7.11.amd64.msi"
      isURL="true"
      destinationPath="${temp.download}"
      destinationFile="python-2-7-11-amd64.msi"
      estimatedSize="19">
    <description>Downloads the python 2.7.11 amd64 msi</description>
  </setupTask>
  <setupTask
      xsi:type="downloadtask:DownloadTask"
      id="download.maven"
      excludedTriggers="STARTUP"
      resourceLocation="http://www-us.apache.org/dist/maven/maven-3/3.3.9/binaries/apache-maven-3.3.9-bin.zip"
      isURL="true"
      destinationPath="${temp.download}"
      destinationFile="maven.zip"
      estimatedSize="8">
    <description>Downloads maven 3.3.9 binaries archive</description>
  </setupTask>
  <setupTask
      xsi:type="unziptask:UnzipTask"
      id="unzip.maven"
      excludedTriggers="STARTUP"
      predecessor="download.maven"
      zipFile="${temp.download}/maven.zip"
      destinationDir="${installation.location}"/>
  <setupTask
      xsi:type="downloadtask:DownloadTask"
      id="download.tomcat"
      excludedTriggers="STARTUP"
      resourceLocation="http://archive.apache.org/dist/tomcat/tomcat-7/v7.0.8/bin/apache-tomcat-7.0.8-windows-x64.zip"
      isURL="true"
      destinationPath="${temp.download}"
      destinationFile="tomcat.zip"
      estimatedSize="9">
    <description>Downloads tomcat 7.0.68 binaries archive</description>
  </setupTask>
  <setupTask
      xsi:type="unziptask:UnzipTask"
      id="unzip.tomcat"
      excludedTriggers="STARTUP"
      predecessor="download.tomcat"
      zipFile="${temp.download}/tomcat.zip"
      destinationDir="${installation.location}">
    <description></description>
  </setupTask>
  <setupTask

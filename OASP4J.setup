  <version name="Corp Mars"
      label="corpMars"
      requiredJavaVersion="1.7">
    <setupTask
        xsi:type="downloadtask:DownloadTask"
        id="download.vm"
        excludedTriggers="STARTUP"
        resourceLocation="${resource.location}/vm.zip"
        destinationPath="${temp.download}"
        destinationFile="vm.zip"
        priority="100">
      <description>Copies the java vm</description>
    </setupTask>
    <setupTask
        xsi:type="unziptask:UnzipTask"

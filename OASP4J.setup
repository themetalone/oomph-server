    xsi:schemaLocation="http://www.example.org/DownloadTask/1.0 https://raw.githubusercontent.com/themetalone/oomph-server/master/model/Downloadtask-1.0.ecore http://www.eclipse.org/oomph/setup/jdt/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/JDT.ecore http://www.example.org/UnzipTask/1.0 https://raw.githubusercontent.com/themetalone/oomph-server/master/model/Unziptask-1.0.ecore"
    name="oasp4j"
    label="OASP4J">
  <annotation
      source="http://www.eclipse.org/oomph/setup/BrandingInfo">
    <detail
        key="folderName">
      <value>eclipse</value>
    </detail>
    <detail
        key="folderName.macosx">
      <value>Eclipse</value>
    </detail>
  </annotation>
  <setupTask
      xsi:type="setup:VariableTask"
      type="FOLDER"
      name="installation.root"
      storageURI="scope://Installation"
      label=""/>
  <setupTask
      xsi:type="setup:VariableTask"
      id="installation.location"
      name="installation.location"
      value="${installation.root}/software"
      storageURI="scope://Installation">
    <description></description>
  </setupTask>
  <setupTask
      xsi:type="setup:VariableTask"
      name="resource.location"
      value="C:/oomph/server"/>
  <setupTask
      xsi:type="setup:VariableTask"
      name="workspace.container"
      value="${installation.root}/workspaces"
      storageURI="scope://Installation"
      label=""/>
  <setupTask
      xsi:type="setup:VariableTask"
      name="temp.download"
      value="${installation.root}/temp">
    <description>Specifies where temporary data will be stored</description>
  </setupTask>
  <setupTask
      xsi:type="downloadtask:DownloadTask"
      id="download.scripts"
      excludedTriggers="STARTUP"
      resourceLocation="${resource.location}/scripts.zip"
      destinationPath="${temp.download}"
      destinationFile="scripts.zip"
      priority="100">
    <description>Copies the scripts.zip</description>
  </setupTask>
  <setupTask
      xsi:type="unziptask:UnzipTask"
      id="unzip.scripts"
      excludedTriggers="STARTUP"
      predecessor="download.scripts"
      zipFile="${temp.download}/scripts.zip"
      destinationDir="${installation.root}"
      priority="100">
    <description>Unzips the scripts archive</description>
  </setupTask>
  <setupTask
      xsi:type="downloadtask:DownloadTask"
      id="download.ide"
      excludedTriggers="STARTUP"
      resourceLocation="${resource.location}/ide.zip"
      destinationPath="${temp.download}"
      destinationFile="ide.zip"
      priority="100">
    <description>Copies the ide.zip</description>
  </setupTask>
  <setupTask
      xsi:type="unziptask:UnzipTask"
      id="unzip.ide"
      excludedTriggers="STARTUP"
      predecessor="download.ide"
      filter=""
      zipFile="${temp.download}/ide.zip"
      destinationDir="${installation.root}"
      priority="100">
    <description>Unzips the ide archive</description>
  </setupTask>
  <setupTask
      xsi:type="downloadtask:DownloadTask"
      id="download.ant"
      excludedTriggers="STARTUP"
      resourceLocation="http://www-eu.apache.org/dist//ant/binaries/apache-ant-1.9.6-bin.zip"
      isURL="true"
      readTimeout="1000"
      destinationPath="${temp.download}"
      destinationFile="ant.zip"
      estimatedSize="8">
    <description>Downloads the ant 1.9.6 archive</description>
  </setupTask>
  <setupTask
      xsi:type="unziptask:UnzipTask"
      id="unzip.ant"
      excludedTriggers="STARTUP"
      predecessor="download.ant"
      filter=""
      zipFile="${temp.download}/ant.zip"
      destinationDir="${installation.location}">
    <description>Unzips the ant archive</description>
  </setupTask>
  <setupTask
      xsi:type="downloadtask:DownloadTask"
      id="download.elasticsearch"
      excludedTriggers="STARTUP"
      resourceLocation="https://download.elasticsearch.org/elasticsearch/release/org/elasticsearch/distribution/zip/elasticsearch/2.3.0/elasticsearch-2.3.0.zip"
      isURL="true"
      readTimeout="2000"
      destinationPath="${temp.download}"
      destinationFile="elasticsearch.zip"
      estimatedSize="27">
    <description>Download the elasticsearch 2.3 archive</description>
  </setupTask>
  <setupTask
      xsi:type="unziptask:UnzipTask"
      id="unzip.elasticsearch"
      excludedTriggers="STARTUP"
      predecessor="download.elasticsearch"
      zipFile="${temp.download}/elasticsearch.zip"
      destinationDir="${installation.location}"/>
  <setupTask
      xsi:type="downloadtask:DownloadTask"
      id="download.logstash"
      excludedTriggers="STARTUP"
      resourceLocation="https://download.elastic.co/logstash/logstash/logstash-2.3.0.zip"
      isURL="true"
      destinationPath="${temp.download}"
      destinationFile="logstash.zip"
      estimatedSize="77">
    <description>Downloads logstash 2.3</description>
  </setupTask>
  <setupTask
      xsi:type="unziptask:UnzipTask"
      id="unzip.logstash"
      excludedTriggers="STARTUP"
      predecessor="download.logstash"
      zipFile="${temp.download}/logstash.zip"
      destinationDir="${installation.location}"/>
  <setupTask
      xsi:type="downloadtask:DownloadTask"
      id="download.nodejs"
      excludedTriggers="STARTUP"
      resourceLocation="https://nodejs.org/dist/v4.4.2/win-x64/node.exe"
      isURL="true"
      destinationPath="${temp.download}"
      destinationFile="nodejs-4-4-2-x64.exe"
      estimatedSize="14">
    <description>Downloads the nodejs 4.4.2  x64 exe</description>
  </setupTask>
  <setupTask
      xsi:type="downloadtask:DownloadTask"
      id="download.python"
      excludedTriggers="STARTUP"
      resourceLocation="https://www.python.org/ftp/python/2.7.11/python-2.7.11.amd64.msi"
      isURL="true"
      destinationPath="${temp.download}"
      destinationFile="python-2-7-11-amd64.msi"
      estimatedSize="19">
    <description>Downloads the python 2.7.11 amd64 msi</description>
  </setupTask>
  <setupTask
      xsi:type="downloadtask:DownloadTask"
      id="download.maven"
      excludedTriggers="STARTUP"
      resourceLocation="http://www-us.apache.org/dist/maven/maven-3/3.3.9/binaries/apache-maven-3.3.9-bin.zip"
      isURL="true"
      destinationPath="${temp.download}"
      destinationFile="maven.zip"
      estimatedSize="8">
    <description>Downloads maven 3.3.9 binaries archive</description>
  </setupTask>
  <setupTask
      xsi:type="unziptask:UnzipTask"
      id="unzip.maven"
      excludedTriggers="STARTUP"
      predecessor="download.maven"
      zipFile="${temp.download}/maven.zip"
      destinationDir="${installation.location}"/>
  <setupTask
      xsi:type="downloadtask:DownloadTask"
      id="download.tomcat"
      excludedTriggers="STARTUP"
      resourceLocation="http://www-eu.apache.org/dist/tomcat/tomcat-7/v7.0.68/bin/apache-tomcat-7.0.68.zip"
      isURL="true"
      destinationPath="${temp.download}"
      destinationFile="tomcat.zip"
      estimatedSize="9">
    <description>Downloads tomcat 7.0.68 binaries archive</description>
  </setupTask>
  <setupTask
      xsi:type="unziptask:UnzipTask"
      id="unzip.tomcat"
      excludedTriggers="STARTUP"
      predecessor="download.tomcat"
      zipFile="${temp.download}/tomcat.zip"
      destinationDir="${installation.location}">
    <description></description>
  </setupTask>

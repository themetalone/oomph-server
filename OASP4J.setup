  <version name="mars"
      label="Mars Windows"
      requiredJavaVersion="1.7">
    <setupTask
        xsi:type="setup:VariableTask"
        id="installation.location.win"
        name="installation.location.win"
        value="${installation.root}\software"
        storageURI="scope://Installation">
      <description></description>
    </setupTask>
    <setupTask
        xsi:type="setup:VariableTask"
        name="workspace.container"
        value="${installation.root}\workspaces"
        storageURI="scope://Installation"
        label=""/>
    <setupTask
        xsi:type="setup:VariableTask"
        id=""
        name="temp.download.win"
        value="${installation.root}\temp"
        storageURI="scope://Installation">
      <description>Specifies where temporary data will be stored</description>
    </setupTask>
    <setupTask
        xsi:type="setup:VariableTask"
        name="oasp.settings"
        value="${workspace.container}\main"/>
    <setupTask
        xsi:type="setup:VariableTask"
        id="path.maven"
        name="path.maven"
        value="${installation.location.win}\maven\bin\mvn"
        storageURI="scope://Installation"/>
    <setupTask
        xsi:type="cmd:CmdTask"
        id="rename.ant"
        excludedTriggers="STARTUP"
        predecessor="unzip.ant"
        command="rename">
      <argument>${installation.location.win}\apache-ant-1.9.7</argument>
      <argument>ant</argument>
    </setupTask>
    <setupTask
        xsi:type="cmd:CmdTask"
        id="rename.maven"
        excludedTriggers="STARTUP"
        predecessor="unzip.maven"
        command="rename">
      <argument>${installation.location.win}\apache-maven-3.3.9</argument>
      <argument>maven</argument>
    </setupTask>
    <setupTask
        xsi:type="cmd:CmdTask"
        id="rename.tomcat"
        excludedTriggers="STARTUP"
        predecessor="unzip.tomcat"
        command="rename">
      <argument>${installation.location.win}\apache-tomcat-7.0.8</argument>
      <argument>tomcat</argument>
      <description></description>
    </setupTask>
    <setupTask
        xsi:type="cmd:CmdTask"
        id="rename.elasticsearch"
        excludedTriggers="STARTUP"
        predecessor="unzip.elasticsearch"
        command="rename">
      <argument>${installation.location.win}\elasticsearch-2.3.0</argument>
      <argument>elasticsearch</argument>
    </setupTask>
    <setupTask
        xsi:type="cmd:CmdTask"
        id="rename.logstash"
        excludedTriggers="STARTUP"
        predecessor="unzip.logstash"
        command="rename">
      <argument>${installation.location.win}\logstash-2.3.0</argument>
      <argument>logstash</argument>
      <description>rename.logstash</description>
    </setupTask>
    <setupTask
        xsi:type="cmd:CmdTask"
        id="mkdir.oasp.settings"
        excludedTriggers="STARTUP"
        command="mkdir">
      <argument>${workspace.container}\main\development</argument>
    </setupTask>
    <setupTask
        xsi:type="cmd:CmdTask"
        id="move.oasp.settings"
        excludedTriggers="STARTUP"
        predecessor="unzip.oasp mkdir.oasp.settings"
        command="move">
      <argument>${temp.download.win}\oasp4j-ide-master\oasp4j-ide-settings\src\main\settings</argument>
      <argument>${oasp.settings}\development\</argument>
    </setupTask>
    <setupTask
        xsi:type="cmd:CmdTask"
        id="xcopy.oasp.scripts"
        excludedTriggers="STARTUP"
        predecessor="unzip.oasp"
        command="xcopy">
      <argument>${temp.download.win}\oasp4j-ide-master\oasp4j-ide-scripts\src\main\resources\*</argument>
      <argument>${installation.root}</argument>
      <argument>/E</argument>
    </setupTask>
    <setupTask
        xsi:type="cmd:CmdTask"
        id="package.oasp"
        excludedTriggers="STARTUP"
        predecessor="unzip.oasp"
        command="chdir">
      <argument>${temp.download.win}\oasp4j-ide-master\oasp4j-ide-eclipse-configurator\</argument>
      <argument>&amp;&amp;</argument>
      <argument>${path.maven}</argument>
      <argument>package</argument>
      <argument>-B</argument>
    </setupTask>
    <setupTask
        xsi:type="cmd:CmdTask"
        id="move.oasp.eclipse"
        excludedTriggers="STARTUP"
        predecessor="package.oasp"
        command="move">
      <argument>${temp.download.win}\oasp4j-ide-master\oasp4j-ide-eclipse-configurator\target\oasp4j-ide-eclipse-configurator-1.4.0-SNAPSHOT.jar</argument>
      <argument>${installation.root}\scripts\</argument>
    </setupTask>
    <setupTask
        xsi:type="cmd:CmdTask"
        id="mkdir.software.java"
        command="mkdir">
      <argument>${installation.location.win}\java\</argument>
    </setupTask>
    <setupTask
        xsi:type="cmd:CmdTask"
        id="xcopy.java"
        excludedTriggers="STARTUP"
        command="xcopy">
      <argument>${jre.location-1.7}\*</argument>
      <argument>${installation.location.win}\java\</argument>
      <argument>/E</argument>
      <argument>/I</argument>
      <argument>/Y</argument>
    </setupTask>
    <setupTask
        xsi:type="downloadtask:DownloadTask"
        id="download.help"
        resourceLocation="https://raw.githubusercontent.com/themetalone/oomph-server/master/help.bat"
        isURL="true"
        destinationPath="${installation.root}"
        destinationFile="help.bat"/>
    <setupTask
        xsi:type="cmd:CmdTask"
        id="update.all.workspaces"
        predecessor="xcopy.oasp.scripts move.oasp.settings move.oasp.eclipse xcopy.java download.help"
        command="chdir">
      <argument>${installation.root}\</argument>
      <argument>&amp;&amp;</argument>
      <argument>help.bat</argument>
    </setupTask>
    <description>OASP4J for Mars.</description>
  </version>
  <description>OASP4J provides cool stuff.</description>
</setup:Product>
